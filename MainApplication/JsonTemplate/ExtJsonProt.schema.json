{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Schema für Import von Protokollen",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "Label": {
        "type": "number"
      },
      "Vars": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "Name": {
              "type": "string"
            },
            "Type": {
              "type": "string"
            },
            "Value": {
              "type": "string"
            }
          },
          "required": [
            "Name",
            "Type",
            "Value"
          ]
        }
      }
    },
    "required": [
      "Label",
      "Vars"
    ]
  }
}